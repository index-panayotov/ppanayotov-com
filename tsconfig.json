{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"incremental": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"paths": { "@/*": ["./*"] },
		"plugins": [{ "name": "next" }],
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": false,
		"strictNullChecks": true,
		"target": "ES2017"
	},
	"exclude": ["node_modules"],
	"include": [
		".next/dev/types/**/*.ts",
		".next/types/**/*.ts",
		"**/*.ts",
		"**/*.tsx",
		"next-env.d.ts"
	]
}